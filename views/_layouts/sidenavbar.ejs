<!-- BEGIN: Header-->
<header class="page-topbar" id="header">
  <div class="navbar navbar-fixed">
    <nav class="navbar-main navbar-color nav-collapsible sideNav-lock navbar-light">
      <div class="nav-wrapper">
        <ul class="navbar-list right">
          <li>
            <a class="waves-effect waves-block waves-light profile-button" href="javascript:void(0);" data-target="profile-dropdown">
              <span class="avatar-status">
                <% if (typeof photo != 'undefined') { %>
                <img src="<%= photo %>" alt="avatar" onerror="this.onerror=null; this.src='/uploads/default_user.jpg';">
                <% } else { %>
                <img src="/uploads/default_user.jpg" alt="avatar">
                <% } %>
              </span>
            </a>
          </li>
        </ul>
        <ul class="dropdown-content" id="profile-dropdown">
          <li>
            <a class="grey-text text-darken-1" href="/profile"><i class="material-icons">person_outline</i>Profile</a>
          </li>
          <li>
            <a class="grey-text text-darken-1" href="/changepass"><i class="material-icons">lock_outline</i>Change Password</a>
          </li>
          <li>
            <a class="grey-text text-darken-1" href="/logout" onclick="return confirm('Are you sure?');"><i class="material-icons">keyboard_tab</i>Logout</a>
          </li>
        </ul>
      </div>
    </nav>
  </div>
</header>
